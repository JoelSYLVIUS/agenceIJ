{% extends  '::base.html.twig' %}

{% block title %}Bienvenue sur AIJ !{% endblock %}

{% block stylesheets %}
<style>
h1, p{
        margin-top: 150px;
}
h1{
        font-size: 100px;
        text-shadow: 2px 2px  1px #ffffff;
}
p{
        font-size: 30px;
        text-shadow: 2px 2px  1px #ffffff;
}
section{
        min-height:75vh;
        padding-top: 5vh;
}
.card{
        height: 350px;
}
.parallax-container {
      height: 80vh;
    }
</style>
{% endblock %}
{% block body %}

<div class="parallax-container">
      <div class="parallax"><img src="{{ asset('medias/snow-leopard-2.jpg') }}"></div>
      <div class="container center">
              <h1>{{titre}}</h1>
              <p> {{slogan}}</p>
      </div>
</div>
<section>
        <div class="container">
                <div class="row">
                        <article class="col s4">
                                <div class="row">
                                        <div class="col s12">
                                                <div class="card hoverable">
                                                        <div class="card-content">
                                                                <span class="card-title"><h2>Univers de MARQUE</h2></span>
                                                                <ul class="center">
                                                                        <li>IDENTITÉ VISUELLE</li>
                                                                        <li>CHARTE GRAPHIQUE</li>
                                                                        <li>VISUELS PUBLICITAIRES</li>
                                                                </ul>
                                                        </div>
                                                </div>
                                        </div>
                                </div>
                        </article>
                        <article class="col s4">
                                <div class="row">
                                        <div class="col s12">
                                                <div class="card hoverable">
                                                        <div class="card-content">
                                                                <span class="card-title"><h2>Créations WEB</h2></span>
                                                                <ul class="center">
                                                                        <li>WEBDESIGN</li>
                                                                        <li>DÉVELOPPEMENT</li>
                                                                        <li>SITE VITRINE</li>
                                                                        <li>SITE CORPORATE</li>
                                                                        <li>SITE E-COMMERCE</li>
                                                                </ul>
                                                        </div>
                                                </div>
                                        </div>
                                </div>
                        </article>
                        <article class="col  s4">
                                <div class="row">
                                        <div class="col s12">
                                                <div class="card hoverable">
                                                        <div class="card-content">
                                                                <span class="card-title"><h2>Stratégie DIGITALE</h2></span>
                                                                <ul class="center">
                                                                        <li>SEO</li>
                                                                        <li>BANNIÈRES, EMAILINGS</li>
                                                                        <li>MOTION DESIGN</li>
                                                                </ul>
                                                        </div>
                                                </div>
                                        </div>
                                </div>
                        </article>
                </div>
                <div class="row">
                        <article class="col s4">
                                <div class="row">
                                        <div class="col s12">
                                                <div class="card hoverable">
                                                        <div class="card-content">
                                                                <span class="card-title"><h2>Supports PRINT</h2></span>
                                                                <ul class="center">
                                                                        <li>PLAQUETTE</li>
                                                                        <li>CATALOGUE</li>
                                                                        <li>AFFICHAGE</li>
                                                                        <li>ROLL-UP</li>
                                                                </ul>
                                                        </div>
                                                </div>
                                        </div>
                                </div>
                        </article>
                </div>
        </div>
</section>


{% endblock %}

{% block javascripts %}
<script>
$(document).ready(function(){
      $('.parallax').parallax();
    });

    </script>

{% endblock %}
